                	label3.setVisible(true);
                    elapsedTime++;
                    hours = elapsedTime / 3600;
                    minutes = (elapsedTime % 3600) / 60;
                    seconds = elapsedTime % 60;
                    
                    if (seconds <= 9) {
                        label3.setText("Time: " + minutes + ":0" + seconds);
                    }
                    
                    else {
                        label3.setText("Time: " + minutes + ":" + seconds);
                    }
                    
                    if(hours >= 1) {
                    	
                    	if(minutes <= 9 && seconds <= 9) {
                    		label3.setText("Time: " + hours + ":0" + minutes + ":0" + seconds);
                    	}
                    	
                    	else if(minutes <= 9 && seconds >= 10) {
                    		label3.setText("Time: " + hours + ":0" + minutes + ":" + seconds);
                    	}
                    	
                    	else if(minutes >= 10 && seconds <= 9) {
                    		label3.setText("Time: " + hours + ":" + minutes + ":0" + seconds);
                    	}
                    	
                    	else if(minutes >= 10 && seconds >= 10) {
                    		label3.setText("Time: " + hours + ":" + minutes + ":" + seconds);
                    	}
                    	
