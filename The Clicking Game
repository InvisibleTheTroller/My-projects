                            }
                            
                            if(counts.get() >= 100000000 && counts.get() < 250000000 && autoClicking) {
                            	button19.doClick();
                            }
                            
                            else {
                            }
                            
                            if(counts.get() >= 250000000 && counts.get() < 500000000 && autoClicking) {
                            	button20.doClick();
                            }
                            
                            else {
                            }
                            
                            if(counts.get() >= 500000000 && counts.get() < 1000000000 && autoClicking) {
                            	button21.doClick();
                            }
                            
                            else {
                            }
                            
                            if(counts.get() >= 1000000000) {
                            	button22.doClick();
                            	isClicked = true;
                            	autoTimer.stop();
                            }
                            
                            else {
                            }
                            
                        });
                        autoTimer.start();
                        
                        if(isClicked) {
                        	autoTimer.stop();
                        }
                        
                    }
                    
                    else {
                    	autoButton.setText("Auto leveling: Off");
                    	autoTimer.stop();
                    	if(counts.get() >= 20) {
                            	button2.setEnabled(true);
                            }
                            
                            else {
                            	button2.setEnabled(false);
                            }
                            
                            if(counts.get() >= 100) {
                            	button3.setEnabled(true);
                            }
                            
                            else {
                            	button3.setEnabled(false);
                            }
                            
                            if(counts.get() >= 500) {
                            	button4.setEnabled(true);
                            }
                            
                            else {
                            	button4.setEnabled(false);
                            }
                            
                            if(counts.get() >= 1000) {
                            	button5.setEnabled(true);
                            }
                            
                            else {
                            	button5.setEnabled(false);
                            }
                            
                            if(counts.get() >= 5000) {
                            	button6.setEnabled(true);
                            }
                            
                            else {
                            	button6.setEnabled(false);
                            }
                            
                            if(counts.get() >= 15000) {
                            	button7.setEnabled(true);
                            }
                            
                            else {
                            	button7.setEnabled(false);
                            }
                            
                            if(counts.get() >= 30000) {
                            	button8.setEnabled(true);
                            }
                            
                            else {
                            	button8.setEnabled(false);
                            }
                            
                            if(counts.get() >= 50000) {
                            	button9.setEnabled(true);
                            }
                            
                            else {
                            	button9.setEnabled(false);
                            }
                            
                            if(counts.get() >= 100000) {
                            	button10.setEnabled(true);
                            }
                            
                            else {
                            	button10.setEnabled(false);
                            }
                            
                            if(counts.get() >= 200000) {
                            	button11.setEnabled(true);
                            }
                            
                            else {
                            	button11.setEnabled(false);
                            }
                            
                            if(counts.get() >= 500000) {
                            	button12.setEnabled(true);
                            }
                            
                            else {
                            	button12.setEnabled(false);
                            }
                            
                            if(counts.get() >= 1000000) {
                            	button13.setEnabled(true);
                            }
                            
                            else {
                            	button13.setEnabled(false);
                            }
                            
                            if(counts.get() >= 2500000) {
                            	button14.setEnabled(true);
                            }
                            
                            else {
                            	button14.setEnabled(false);
                            }
                            
                            if(counts.get() >= 5000000) {
                            	button15.setEnabled(true);
                            }
                            
                            else {
                            	button15.setEnabled(false);
                            }
                            
                            if(counts.get() >= 10000000) {
                            	button16.setEnabled(true);
                            }
                            
                            else {
                            	button16.setEnabled(false);
                            }
                            
                            if(counts.get() >= 25000000) {
                            	button17.setEnabled(true);
                            }
                            
                            else {
                            	button17.setEnabled(false);
                            }
                            
                            if(counts.get() >= 50000000) {
                            	button18.setEnabled(true);
                            }
                            
                            else {
                            	button18.setEnabled(false);
                            }
                            
                            if(counts.get() >= 100000000) {
                            	button19.setEnabled(true);
                            }
                            
                            else {
                            	button19.setEnabled(false);
                            }
                            
                            if(counts.get() >= 250000000) {
                            	button20.setEnabled(true);
                            }
                            
                            else {
                            	button20.setEnabled(false);
                            }
                            
                            if(counts.get() >= 500000000) {
                            	button21.setEnabled(true);
                            }
                            
                            else {
                            	button21.setEnabled(false);
                            }
                            
                            if(counts.get() >= 1000000000) {
                            	button22.setEnabled(true);
                            }
                            
                            else {
                            	button22.setEnabled(false);
                            }
                            
                    }
                    autoClicking = !autoClicking;
                    
                });
        		
        	});
        	
        	gameStartTimer.setRepeats(false);
        	gameStartTimer.start();
        	
        });
          
    }
}
